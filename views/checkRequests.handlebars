{{#each data.userRequests}}
<div class="py-3 col-md-12">
  <div class="cenAlign ctxt insideBorder btnBg">
    <div class="">
      <h3 class="" name="seedOffer-seedName">{{username}}</h3>
      <p class="" name="seedDescription">{{mailing}}</p>
    </div>
    {{#if alreadySent}}
    <div>
      <button type="submit" name="button to click for the user to request this seedOffer" class="btn btn-info accountBtns"
        data-offerid="{{id}}">Sent</button>
    </div>
    {{else}}
    <div>
      <button type="submit" name="button to click for the user to request this seedOffer" class="btn btn-info accountBtns"
        data-offerid="{{id}}">Inform Sent</button>
    </div>
    {{/if}}
  </div>
</div>
{{/each}}