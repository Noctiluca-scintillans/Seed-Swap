<div class="py-3 col-md-12">
  <div class="cenAlign ctxt insideBorder btnBg">
    <div class="">
      <h3 class="border-below" name="seedOffer-seedName">{{seedName}}</h3>
      <p class="" name="seedDescription">{{offerDescription}}</p>
    </div>
    <div>
      <a href="/offers/checkRequests/{{webLink}}">
      <button type="submit" name="button to click for the user to request this seedOffer" class="btn btn-info accountBtns" data-weblink="{{webLink}}">View Request</button>
      </a>
      <button type="delete" name="button to delete this seedOffer" class="btn btn-info accountBtns"
      data-offerid="{{id}}"
      >Delete This Offer</button>  
    </div>
  </div>
</div>